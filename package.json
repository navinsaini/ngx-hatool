{
  "name": "ngx-hatool",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve ngx-hatool-tester",
    "lint": "ng lint",
    "build_lib": "ng build ngx-hatool --configuration production",
    "watch_lib": "ng build ngx-hatool --watch",
    "copy-license": "cp ./LICENSE ./dist/ngx-hatool",
    "copy-readme": "cp ./README.md ./dist/ngx-hatool",
    "copy-files": "npm run copy-license && npm run copy-readme",
    "npm_pack": "cd dist/ngx-hatool && npm pack",
    "package": "npm run build_lib",
    "publish": "cd dist/ngx-hatool && npm publish"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~13.0.0",
    "@angular/common": "~13.0.0",
    "@angular/compiler": "~13.0.0",
    "@angular/core": "~13.0.0",
    "@angular/forms": "~13.0.0",
    "@angular/platform-browser": "~13.0.0",
    "@angular/platform-browser-dynamic": "~13.0.0",
    "@angular/router": "~13.0.0",
    "highlight.js": "^11.3.1",
    "ngx-highlightjs": "^6.1.0",
    "rxjs": "~6.5.4",
    "tslib": "^2.3.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~13.0.3",
    "@angular/cli": "~13.0.3",
    "@angular/compiler-cli": "~13.0.0",
    "@types/highlight.js": "^10.1.0",
    "@types/jasmine": "^3.10.2",
    "@types/jasminewd2": "^2.0.10",
    "@types/node": "^12.11.1",
    "codelyzer": "^6.0.2",
    "eslint": "^8.3.0",
    "jasmine-core": "^3.10.1",
    "jasmine-spec-reporter": "^7.0.0",
    "karma": "^6.3.9",
    "karma-chrome-launcher": "^3.1.0",
    "karma-coverage-istanbul-reporter": "^3.0.3",
    "karma-jasmine": "^4.0.1",
    "karma-jasmine-html-reporter": "^1.7.0",
    "ng-packagr": "^13.0.8",
    "protractor": "^7.0.0",
    "ts-node": "^10.4.0",
    "typescript": "~4.4.3"
  }
}
